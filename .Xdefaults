Rxvt.perl-ext-common:default, selection-to-clipboard, clipboard 
URxvt*imLocale: en_US.UTF-8
URxvt.font:xft:Mononoki NF:size=11:antialias:true
URxvt.scrollBar:false
URxvt.clipboard.autocopy: true
URxvt.keysym.Shift-Control-V:eval:paste_clipboard
URxvt.keysym.Shift-Control-C:eval:selection_to_clipboard
URxvt.keysym.C-equal:font-size:increase
URxvt.keysym.C-minus:font-size:decrease
URxvt.iso14755:false
URxvt.iso14755_52:false
Xcursor.theme:Breeze Light
XTerm*vt100.utf8:true
XTerm*termName: xterm-256color
XTerm*vt100.metaSendsEscape: true
XTerm*vt100.saveLines: 4096
XTerm*vt100.renderFont: true
XTerm*vt100.locale:true
XTerm.vt100.bellIsUrgent: false
XTerm*vt100.faceName:xft:Mononoki NF:size=11:antialias=true
XTerm*letterSpace: -1
XTerm*vt100.scrollBar:false
xterm*visualBell:off
*background:#282828
*foreground:#ebdbb2
*color0:#100e23
*color1:#cc241d
*color2:#98971a
*color3:#d79921
*color4:#458588
*color5:#b16286
*color6:#689d6a
*color7:#a89984
*color8:#565575
*color9:#fb4934
*color10:#b8bb26
*color11:#fabd2f
*color12:#83a598
*color13:#d3869b
*color14:#8ec07c
*color15:#fbf1c7
XTerm*vt100.translations: #override \n\
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(d) \n\
    Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

