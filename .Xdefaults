URxvt.perl-ext-common:default, selection-to-clipboard, clipboard, resize-font 
URxvt*imLocale: en_US.UTF-8
URxvt.font:xft:Fira Code:size=10:antialias:true
URxvt.scrollBar:false
URxvt.clipboard.autocopy: true
URxvt.keysym.Shift-Control-V:eval:paste_clipboard
URxvt.keysym.Shift-Control-C:eval:selection_to_clipboard
URxvt.keysym.C-minus:resize-font:smaller
URxvt.keysym.C-equal:resize-font:bigger
URxvt.keysym.C-0:resize-font:reset
URxvt.iso14755:false
URxvt.iso14755_52:false
URxvt.resize-font.step: 1
Xcursor.theme:Breeze Light
XTerm*vt100.utf8:true
XTerm*vt100.geometry: 80x32
XTerm*termName: xterm-256color
XTerm*vt100.metaSendsEscape: true
XTerm*vt100.saveLines: 4096
XTerm*vt100.renderFont: true
XTerm*vt100.locale:true
XTerm.vt100.bellIsUrgent: false
XTerm*vt100.font:xft:DroidSansMono NF:size=10:antialias=true
XTerm*letterSpace: -1
XTerm*vt100.scrollBar:false
xterm*visualBell:off
hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2
XTerm*vt100.translations: #override \n\
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(d) \n\
    Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V: insert-selection(CLIPBOARD)
